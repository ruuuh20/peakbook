

<div class="container">
  <h1>Welcome!</h1>

  <% if user_signed_in? %>
    <h4>You are logged in.</h4>
  <% else %>

  <div class="wrapper">
    <h4>Please log in.</h4>
    <%= link_to "Sign Up", new_user_registration_path %><br>
    <%= link_to "Sign In", new_user_session_path %><br>
    <%= link_to "Sign in with GitHub", user_github_omniauth_authorize_path %>
  </div>
<% end %>
</div>
